<svg width="32" height="32" viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="brokenGradPng" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#e5e5e5;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#cccccc;stop-opacity:1" />
    </linearGradient>
    <filter id="shadowPng" x="-50%" y="-50%" width="200%" height="200%">
      <feDropShadow dx="1" dy="1" stdDeviation="1" flood-color="#000000" flood-opacity="0.4"/>
    </filter>
    <!-- Clippath pour effet brisé version PNG -->
    <clipPath id="brokenClipPng">
      <polygon points="0,0 11,0 13,16 9,32 0,32"/>
      <polygon points="13,0 21,0 23,16 19,32 11,32"/>
      <polygon points="23,0 32,0 32,32 21,32"/>
    </clipPath>
  </defs>
  
  <!-- Fond sombre avec bordure -->
  <rect width="32" height="32" rx="6" fill="#1a1a1a" stroke="#333" stroke-width="0.5"/>
  
  <!-- Icône musicale combinée avec effet brisé -->
  <g clip-path="url(#brokenClipPng)">
    <!-- Note de musique principale -->
    <ellipse cx="10" cy="23" rx="4" ry="2.5" fill="url(#brokenGradPng)" filter="url(#shadowPng)"/>
    <rect x="14" y="10" width="2.5" height="14" fill="url(#brokenGradPng)" filter="url(#shadowPng)"/>
    
    <!-- Croche stylisée -->
    <path d="M 16.5 10 Q 22 8 24 12 Q 22 14 16.5 16" fill="url(#brokenGradPng)" filter="url(#shadowPng)"/>
    
    <!-- Deuxième note plus petite -->
    <ellipse cx="20" cy="25" rx="2.5" ry="1.5" fill="url(#brokenGradPng)" filter="url(#shadowPng)" opacity="0.8"/>
    
    <!-- Lettre B intégrée -->
    <text x="26" y="20" font-family="Arial Black, Arial, sans-serif" font-size="8" font-weight="900" fill="url(#brokenGradPng)" filter="url(#shadowPng)">B</text>
  </g>
  
  <!-- Lignes de fissure subtiles -->
  <line x1="13" y1="0" x2="11" y2="32" stroke="#555" stroke-width="0.5" opacity="0.3"/>
  <line x1="23" y1="0" x2="21" y2="32" stroke="#555" stroke-width="0.5" opacity="0.3"/>
  
  <!-- Effet de lueur -->
  <rect width="32" height="32" rx="6" fill="none" stroke="url(#brokenGradPng)" stroke-width="0.5" opacity="0.2"/>
</svg>
